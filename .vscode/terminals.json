{
  "autorun": true,
  "terminals": [
    {
      "name": "Statico",
      "command": "npm run website:clean && npm run website:build && npm run website:watch",
      "persistent": "sd-statico"
    },
    {
      "name": "Svelto",
      "command": "npm run svelto:dev",
      "persistent": "sd-svelto"
    },
    {
      "name": "Server",
      "command": "npm run website:serve",
      "persistent": "sd-server"
    }
  ]
}
